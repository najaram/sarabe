        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-vue": "^4.7.1",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.14.4",
        "prettier": "1.14.3",
        "prettier-eslint": "^8.8.2",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "@coreui/coreui": "^2.0.12",
